{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "emcaVersion": 7,
      "emcafeatures": {
        "jsx": true
      }
    },
    "plugins": [
        "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": 0
    }
}